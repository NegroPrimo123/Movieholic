{
  "name": "movie-recommendation-service",
  "version": "1.0.0",
  "description": "Сервис рекомендаций фильмов по сценариям с использованием API Кинопоиска",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:migrate": "psql -h localhost -U postgres -d movie_recommendations -f migrations/001_create_tables.sql",
    "db:seed": "node seeds/seed.js",
    "db:reset": "npm run docker:down && npm run docker:up && sleep 5 && npm run db:migrate && npm run db:seed",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "movies",
    "recommendations",
    "kinopoisk",
    "nodejs",
    "express",
    "postgres"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "pg": "^8.11.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
